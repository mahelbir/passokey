<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <meta name="robots" content="noindex,nofollow"/>
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <meta name="theme-color" content="#0C85E1">
  <meta name="msapplication-navbutton-color" content="#0C85E1">
  <meta name="mobile-web-app-capable" content="yes">
  <meta name="apple-mobile-web-app-status-bar-style" content="#0C85E1">
  <link rel="apple-touch-icon" sizes="128x128" href="~/assets/img/logo-128.png">
  <link rel="icon" sizes="128x128" href="~/assets/img/logo-128.png">
  <link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" />
  <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined" />
  <link rel="stylesheet" href="~/assets/css/bootstrap.min.css">
  <link rel="stylesheet" href="~/assets/css/sweetalert2.min.css">
  <link rel="stylesheet" href="~/assets/css/site.css" asp-append-version="true">
  <title>@Configuration["AppName"]</title>
</head>
<body>

<div class="content">
  <div class="container-fluid mt-5">
    @RenderBody()
  </div>
</div>

<script src="~/assets/js/jquery.min.js"></script>
<script src="~/assets/js/sweetalert2.min.js"></script>
<script src="~/assets/js/loading.js" asp-append-version="true"></script>
<script src="~/assets/js/common.js" asp-append-version="true"></script>
<script src="~/assets/js/site.js" asp-append-version="true"></script>
<script src="https://unpkg.com/@@simplewebauthn/browser/dist/bundle/index.es5.umd.min.js"></script>
@await RenderSectionAsync("Scripts", required: false)

</body>
</html>